package prac;



import java.util.*;
public class GuGudan {

	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		System.out.println("출력하고 싶은 구구단 수 숫자를 입력해주세요");
		
		int user_input = scanner.nextInt();
		if (user_input > 0 && user_input < 10) {
			System.out.println(user_input*1);
			System.out.println(user_input*2);
			System.out.println(user_input*3);
			System.out.println(user_input*4);
			System.out.println(user_input*5);
			System.out.println(user_input*6);
			System.out.println(user_input*7);
			System.out.println(user_input*8);
			System.out.println(user_input*9);
		}else {
			System.out.println("1-9단까지만 출력가능합니다.");
		}
		
		
	   scanner.close();
		
		
	}

}
