package prac;

import java.util.*;
public class GuGudan_Upgrade {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("출력하고 싶은 구구단을 입력해주세요");
		
		  int user_input= scanner.nextInt();
		  if(user_input>0 && user_input <10) {
	      for(int i=1;i<10;i++) {
	    	  System.out.println(user_input*i);
	      }
			
		 scanner.close();

	}else {
		System.out.println("1-9단까지만 출력 가능합니다");
	}

}
}
